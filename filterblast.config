#	Preset "Leveling CASTER" [GG Leveling CASTER] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Filter/GG%20leveling%20CASTER.filter]
	Preset "Endgame" DEFAULT [GG Endgame] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Filter/GG%20endgame.filter]
#	Preset "MF" [GG MF] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Filter/GG%20MF.filter] 
    
ColorThemes filtertheme

    Theme "Amethyst" [GG Amethyst] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Amethyst.filtertheme]
    Theme "Atlantisite" [GG Atlantisite] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Atlantisite.filtertheme]
	Theme "Bloodstone" [GG Bloodstone] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Bloodstone.filtertheme]
	Theme "Game like" [GG Game like] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Game%20like.filtertheme]
	Theme "Jade" [GG Jade] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Jade.filtertheme]
	Theme "Zoisite" DEFAULT [GG Zoisite] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Color%20Themes/GG%20Zoisite.filtertheme]

SoundThemes

	Theme "Default sound set" DEFAULT [Default sound set] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Sound%20Themes/GG%20Default%20sound%20set.filter]
	Theme "Shaper sound set" [Shaper sound set] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Sound%20Themes/GG%20Shaper%20sound%20set.filter]
	Theme "Oriath sound set" [Oriath sound set] [https://raw.githubusercontent.com/Greengroove/GG-LootFilter/master/Sound%20Themes/GG%20Oriath%20sound%20set.filter]
